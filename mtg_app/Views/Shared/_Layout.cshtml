@model mtg_app.Models.CardsViewModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="~/wwwroot/site.css">
    <!-- Styles.Render("~/wwwroot/css")-->


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a  href="/store" ><img src="~/themes/images/proto.png" alt="Bootsshop" /></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    <li>@Html.ActionLink("Store->RARITY", "Index", "Store")</li>
                    <li>@Html.ActionLink("Browse", "Browse", "Store", new {rarity = "U"})</li>
                    <!-- NAME DISPLAYING / Controller / View -->
                    <li>@Html.ActionLink("StoreManager", "Index", "StoreManager")</li>


                </ul>
  
            </div>
        </div>
    </div>
    <br /><br />
    
       
    <div class="container-fluid body-content">
        @RenderBody()
        
        <hr />
        <!--
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>-->
    </div>

    @RenderSection("scripts", required: false)
    <script src="~/Scripts/action.js">

    </script>
</body>
</html>