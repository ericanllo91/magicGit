@model mtg_app.Models.CardsViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <h1>Item is:TEST</h1>

    <h2>NOT WORKING</h2>
    <ul>
        @foreach (var item in Model.Cards)
        {
            @if (item.Price != null){
                <li>@item.Name</li>
                <li>@item.Multiverse_id</li>
                <li>@item.Price</li>
                <img src="@item.Url">
            }
            
        }
    </ul>
</body>
</html>
